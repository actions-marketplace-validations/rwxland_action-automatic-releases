{
  "name": "automatic-releases",
  "version": "2.0.1",
  "private": true,
  "description": "Generate automatic releases whenever changes land on master",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramonrwx/gh-automatic-releases"
  },
  "scripts": {
    "test": "jest --env=node --colors --verbose true",
    "build": "webpack --config webpack.config.js",
    "clean": "rm -rf node_modules npm-error.log dist",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --max-warnings=0",
    "lint:prettier": "prettier --check .",
    "lintfix": "npm run lintfix:eslint && npm run lintfix:prettier",
    "lintfix:eslint": "eslint --fix .",
    "lintfix:prettier": "prettier --write ."
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "2.0.0",
    "@octokit/rest": "16.36.0",
    "conventional-changelog-angular": "^5.0.12",
    "conventional-commits-parser": "^3.2.0",
    "globby": "^11.0.1",
    "md5-file": "^5.0.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-jest": "^29.6.1",
    "encoding": "^0.1.13",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jest": "^29.6.1",
    "jest-circus": "^29.6.1",
    "nock": "^13.3.1",
    "portfinder": "^1.0.28",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "which": "^3.0.1"
  }
}
